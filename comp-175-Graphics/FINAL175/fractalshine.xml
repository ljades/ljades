<scenefile>
<globaldata>
  <diffusecoeff v="0.5"/>
  <specularcoeff v="0.5"/>
  <ambientcoeff v="0.5"/>
</globaldata>

<cameradata>
  <pos x="0" y="0" z="4"/>
  <up x="0" y="1" z="0"/>
  <focus x="0" y="0" z="0"/>
  <heightangle v="30"/>
</cameradata>

	<lightdata>
		<id v="0"/>
		<color r=".3" g=".3" b=".3"/>
		<position x="0" y="0" z="4"/>
	</lightdata>

<object type="tree" name="root">
  <transblock>
	<scale x="1.5" y="1.5" z="1.5" />
    <object type="primitive" name="sphere" >
	  <ambient r=".2" g=".2" b=".2"/>
      <specular r=".3" g=".3" b=".3"/>
      <reflective r=".8" g=".8" b=".8"/>
      <diffuse r="0.8" g="0.6" b="0.6"/>
      <shininess value="8"/>
    </object>
  </transblock>

  <transblock>
	<scale x="12" y="12" z="12" />
    <object type="primitive" name="cube" >
	  <ambient r=".2" g=".2" b=".2"/>
      <specular r=".3" g=".3" b=".3"/>
      <reflective r=".8" g=".8" b=".8"/>
      <diffuse r="0.6" g="0.6" b="0.8"/>
      <shininess value="8"/>
    </object>
  </transblock>


</object>
</scenefile>
